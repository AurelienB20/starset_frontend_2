{
  "expo": {
    "name": "starset",
    "slug": "frontend_starset_2",
    "owner": "starset77",

    "version": "1.0.2",

    "orientation": "portrait",
    "icon": "./assets/images/starset-icon.png",
    "scheme": "starset",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.starset.frontend",
      "ios.buildNumber": "0.2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "Cette application a besoin d'accéder à votre galerie pour sélectionner des documents.",
        "NSCameraUsageDescription": "Cette application a besoin d'accéder à votre caméra pour prendre des photos.",
        "NSPhotoLibraryAddUsageDescription": "Cette application a besoin d'accéder à votre galerie pour enregistrer des fichiers.",
        "NSLocationWhenInUseUsageDescription": "Cette application peut utiliser votre position si vous activez la localisation dans la carte.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Cette application peut utiliser votre position si vous activez la localisation dans la carte.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {
            "api.starsetfrance.com": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSIncludesSubdomains": true
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/starset-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.starset77.frontend_starset_2",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-camera",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/starset-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
      
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "141e042a-184a-4b5c-a3be-c7f50835d0b7"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/141e042a-184a-4b5c-a3be-c7f50835d0b7"
    }
  }
}
